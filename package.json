{
  "name": "jflay-website",
  "version": "1.1.3",
  "description": "personal website",
  "main": "./src/index.js",
  "scripts": {
    "dist": "rm -rf dist && NODE_ENV=production webpack --mode production",
    "deploy-patch": "npm run dist && git commit -am \"Deploy site\" && npm version patch && git push",
    "deploy-minor": "npm run dist && git commit -am \"Deploy site\" && npm version minor && git push",
    "deploy-major": "npm run dist && git commit -am \"Deploy site\" && npm version major && git push",
    "start": "webpack-dev-server --mode development --open",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jflayhart/website.git"
  },
  "author": "Josh Flayhart",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jflayhart/website/issues"
  },
  "homepage": "https://github.com/jflayhart/website",
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/fileMock.js"
    }
  },
  "dependencies": {
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "styled-components": "4.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.7.1",
    "babel-loader": "8.0.5",
    "eslint": "5.16.0",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.7.1",
    "react-test-renderer": "16.8.6",
    "url-loader": "1.1.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1"
  }
}
