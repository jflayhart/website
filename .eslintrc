{
  "extends": ["standard", "standard-react"],
  "rules": {
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "never",
    }],
    "jsx-quotes": ["error", "prefer-double"]
  }
}